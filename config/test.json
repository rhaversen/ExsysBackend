{
	"apiLimiter": {
		"sensitivity": {
			"veryLow": {
				"max": 1000,
				"windowMs": 1,
				"standardHeaders": true
			},
			"low": {
				"max": 1000,
				"windowMs": 1,
				"standardHeaders": true
			},
			"medium": {
				"max": 1000,
				"windowMs": 1,
				"standardHeaders": true
			},
			"high": {
				"max": 1000,
				"windowMs": 1,
				"standardHeaders": true
			},
			"critical": {
				"max": 1000,
				"windowMs": 1,
				"standardHeaders": true
			}
		}
	},
	"expressPort": 5000,
	"mongoose": {
		"retrySettings": {
			"maxAttempts": 5,
			"interval": 5000
		},
		"options": {
			"retryWrites": "true",
			"w": "majority",
			"appName": "main"
		}
	},
	"bcrypt": {
		"saltRounds": 1
	},
	"cors": {
		"origin": "http://localhost:5000",
		"credentials": true
	},
	"cookieOptions": {
		"httpOnly": true,
		"secure": false,
		"SameSite": "strict"
	},
	"session": {
		"expiry": 2592000000
	},
	"webhook": {
		"cors": {
			"origin": "*",
			"credentials": true
		}
	}
}
